<configuration xmlns:storeSitecoreCountersInApplicationInsights="http://www.sitecore.net/xmlconfig/storeSitecoreCountersInApplicationInsights/">
  <sitecore storeSitecoreCountersInApplicationInsights:require="!false">
    <metricStorage type="Sitecore.Cloud.ApplicationInsights.Storages.AppInsightsStorage, Sitecore.Cloud.ApplicationInsights" />
    <scheduling>
      <agent type="Sitecore.Cloud.ApplicationInsights.PerformanceCounterGrabberAgent, Sitecore.Cloud.ApplicationInsights" method="Run" interval="00:05:00" resolve="true" />
    </scheduling>
    <counterLoader type="Sitecore.Cloud.ApplicationInsights.CounterLoader,Sitecore.Cloud.ApplicationInsights">
      <includedCounters hint="raw:AddCounterToReport">
        <add category="Sitecore.Data" name="Data | Client Data Reads / sec" />
        <add category="Sitecore.Data" name="Data | Client Data Writes / sec" />
        <add category="Sitecore.Data" name="Data | Items Accessed / sec" />
        <add category="Sitecore.Data" name="Data | Physical Reads / sec" />
        <add category="Sitecore.Data" name="Data | Physical Writes / sec" />
        <add category="Sitecore.Caching" name="Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="Cache Hits / sec" />
        <add category="Sitecore.Caching" name="Cache Misses / sec" />
        <add category="Sitecore.Caching" name="Data Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="Data Cache Hits / sec" />
        <add category="Sitecore.Caching" name="Data Cache Misses / sec" />
        <add category="Sitecore.Caching" name="Html Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="Html Cache Hits / sec" />
        <add category="Sitecore.Caching" name="Html Cache Misses / sec" />
        <add category="Sitecore.Caching" name="Path Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="Path Cache Hits / sec" />
        <add category="Sitecore.Caching" name="Path Cache Misses / sec" />
        <add category="Sitecore.Caching" name="Registry Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="Registry Cache Hits / sec" />
        <add category="Sitecore.Caching" name="Registry Cache Misses / sec" />
        <add category="Sitecore.Caching" name="View State Cache Clearings / sec" />
        <add category="Sitecore.Caching" name="View State Cache Hits / sec" />
        <add category="Sitecore.Caching" name="View State Cache Misses / sec" />
        <add category="Sitecore.Caching" name="XslCache Clearings / sec" />
        <add category="Sitecore.Caching" name="XslCache Hits / sec" />
        <add category="Sitecore.Caching" name="XslCache Misses / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Live Interactions Processed / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | History Interactions Processed / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Total Interactions Processed / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Live Aggregation Errors / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | History Aggregation Errors / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Total Aggregation Errors / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Contacts Processed / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Contact Processing Errors / sec" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Batch Size - Live" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Batch Size - History" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Number of Batches Containing Failing Items - Live" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Number of Batches Containing Failing Items - History" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Aggregation Pipeline Time (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Write Time (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Contact Processing Pipeline Time (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Write Time - Primary (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Write Time - Secondary category=Live (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Write Time - Secondary category=History (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Active Interaction Aggregators" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Check Out Time - Live (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Check In Time - Live (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Check Out Time - History (ms)" />
        <add category="Sitecore.Analytics.Aggregation" name="Aggregation | Average Check In Time - History (ms)" />
      </includedCounters>
    </counterLoader>
  </sitecore>
</configuration>
